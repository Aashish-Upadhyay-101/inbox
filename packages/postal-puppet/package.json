{
  "name": "@uninbox/postal-puppet",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@8.6.12",
  "description": "",
  "author": "McPizza0",
  "license": "MIT",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:puppet": "unbuild",
    "dev:puppet": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.3.4",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.6",
    "@types/node": "^20.10.4",
    "bumpp": "^9.2.0",
    "eslint": "^8.55.0",
    "esno": "^4.0.0",
    "lint-staged": "^15.2.0",
    "pnpm": "^8.11.0",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.0.6",
    "vitest": "^1.0.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "dependencies": {
    "puppeteer": "^21.5.2"
  }
}